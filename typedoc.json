{
    "entryPoints": ["./lib/index.ts"],
    "out": "docs",
    "exclude": ["**/node_modules/**"],
    "plugin": ["typedoc-plugin-markdown"],
    "theme": "default",
    "name": "Playwright Testing Core Library",
    "readme": "none",
    "excludePrivate": true,
    "excludeProtected": true,
    "excludeExternals": true,
    "disableSources": true,
    "categorizeByGroup": true,
    "categoryOrder": ["Core", "Types", "Hooks", "Utils", "*"],
    "groupOrder": ["Namespaces", "Classes", "Interfaces", "Functions", "Variables", "*"],
    "sort": ["static-first", "alphabetical"]
  }